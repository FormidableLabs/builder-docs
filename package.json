{
  "name": "builder-docs",
  "version": "0.0.1",
  "description": "Marketing/documentation site for Builder",
  "main": "webpack.config.dev.js",
  "scripts": {
    "test": "npm run lint",
    "dev": "webpack-dev-server --port 3000 --config webpack.config.dev.js",
    "reinstall-builder": "rm -rf node_modules/builder && npm install",
    "webpack-static": "webpack --config webpack.config.static.js --progress --content-base /",
    "copy-assets": "cp -r static build/static",
    "copy-cname": "cp CNAME build/CNAME",
    "build-static": "npm run reinstall-builder && npm run webpack-static && npm run copy-assets && npm run copy-cname",
    "server-static": "http-server build",
    "open-static": "open http://localhost:8080",
    "push-gh-pages": "git subtree push --prefix build origin gh-pages",
    "lint": "eslint *.js components/**/*.jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/builder-docs.git"
  },
  "author": "david.davidson@formidable.com, paula.lavalle@formidable.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/builder-docs/issues"
  },
  "homepage": "https://github.com/FormidableLabs/builder-docs#readme",
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-eslint": "^4.1.8",
    "babel-loader": "6.2.1",
    "builder": "https://github.com/FormidableLabs/builder",
    "clean-webpack-plugin": "0.1.5",
    "ecology": "1.0.4",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^8.0.2",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-react": "^3.16.1",
    "formidable-landers": "0.0.7",
    "http-server": "^0.8.5",
    "raw-loader": "0.5.1",
    "react": "0.14.5",
    "static-site-generator-webpack-plugin": "2.0.1",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0",
    "webpack-stats-plugin": "0.1.1"
  },
  "dependencies": {
    "babel-core": "6.3.26",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.1.4",
    "radium": "^0.16.2",
    "radium-grid": "^1.0.4",
    "react-dom": "0.14.5"
  }
}
