{
  "name": "builder-docs",
  "version": "0.0.1",
  "description": "Marketing/documentation site for Builder",
  "main": "webpack.config.dev.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --port 3000 --config webpack.config.dev.js",
    "webpack-static": "webpack --config webpack.config.static.js --progress --content-base /",
    "copy-assets": "cp -r static build/static",
    "copy-cname": "cp CNAME build/CNAME",
    "build-static": "npm run webpack-static && npm run copy-assets && npm run copy-cname",
    "push-gh-pages": "git subtree push --prefix build origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/builder-docs.git"
  },
  "author": "david.davidson@formidable.com, paula.lavalle@formidable.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/builder-docs/issues"
  },
  "homepage": "https://github.com/FormidableLabs/builder-docs#readme",
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-loader": "6.2.1",
    "builder": "2.4.0",
    "clean-webpack-plugin": "0.1.5",
    "ecology": "1.0.4",
    "formidable-landers": "0.0.5",
    "raw-loader": "0.5.1",
    "react": "0.14.5",
    "static-site-generator-webpack-plugin": "2.0.1",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0",
    "webpack-stats-plugin": "0.1.1"
  },
  "dependencies": {
    "babel-core": "6.3.26",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "radium": "^0.16.2",
    "react-dom": "0.14.5"
  }
}
